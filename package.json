{
  "name": "nest-playlist-sync",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/jyjokokk/nest-playlist-sync.git",
  "author": "jyjokokk <j.j.kokkola@gmail.com> (https://github.com/jyjokokk)",
  "license": "MIT",
  "engines": {
    "node": ">=20.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "pretty-quick --staged",
      "eslint --fix"
    ]
  },
  "scripts": {
    "prebuild": "rm -rf dist/*",
    "build": "tsc",
    "start": "yarn prebuild && yarn build && node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:watch:cov": "jest --watch --coverage",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
